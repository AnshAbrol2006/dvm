ute; top: 27px; width: 11px; }

.loading-dots > div:first-child { animation: 0.6s ease 0s infinite normal n=
one running loading-dots-in; left: 6px; }

.loading-dots > div:nth-child(2) { animation: 0.6s ease 0s infinite normal =
none running loading-dots-slide; left: 6px; }

.loading-dots > div:nth-child(3) { animation: 0.6s ease 0s infinite normal =
none running loading-dots-slide; left: 26px; }

.loading-dots > div:nth-child(4) { animation: 0.6s ease 0s infinite normal =
none running loading-dots-out; left: 45px; }

@keyframes loading-dots-in {=20
  0% { transform: scale(0); }
  100% { transform: scale(1); }
}

@keyframes loading-dots-slide {=20
  0% { transform: translate(0px); }
  100% { transform: translate(19px); }
}

@keyframes loading-dots-out {=20
  0% { transform: scale(1); }
  100% { transform: scale(0); }
}

